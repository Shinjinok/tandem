<?xml version="1.0"?>

<PropertyList>

  <generic>

    <!-- template
	 <chunk>
	   <name></name>
	   <type>double</type>
	   <node></node>
	 </chunk> 
	 -->

    <input>
      <binary_mode>true</binary_mode>

      <!-- ##### Flight Controls -->
      <chunk>
	<name>aileron</name>
	<type>float</type>
	<node>/controls/flight/aileron</node>
      </chunk> 

      <chunk>
	<name>elevator</name>
	<type>float</type>
	<node>/controls/flight/elevator</node>
      </chunk> 

	  <chunk>
	<name>throttle</name>
	<type>float</type>
	<node>/controls/engines/engine[0]/throttle</node>
      </chunk> 

      <chunk>
	<name>rudder</name>
	<type>float</type>
	<node>/controls/flight/rudder</node>
      </chunk> 

       <chunk>
	<name>collective</name>
	<type>float</type>
	<node>/controls/engines/engine[0]/propeller-pitch</node>
      </chunk>

    </input>

    <output>
      <binary_mode>true</binary_mode>

      <!-- ##### time : 1 doubles ##### -->
	  <chunk>
	<name>timestamp</name>
	<type>double</type>
	<node>/sim/time/steady-clock-sec</node>
      </chunk> 
	 <!-- ##### imu_angular_velocity_rpy : 3 doubles ##### -->
 	

 	<chunk>
		<name>ch1</name>
		<type>double</type>
		<node>/controls/flight/flaps</node>
     </chunk> 

	 <chunk>
		<name>ch2</name>
		<type>double</type>
		<node>/controls/gear/brake-right</node>
     </chunk> 

	<chunk>
		<name>ch3</name>
		<type>double</type>
		<node>/controls/flight/wings</node>
     </chunk> 

    <chunk>
		<name>ch4</name>
		<type>double</type>
		<node>/controls/gear/brake-left</node>
    </chunk> 



	<!-- ##### imu_linear_acceleration_xyz 3doubles ##### -->
      <chunk>
			<name>x-accel</name>
			<type>double</type>
			<node>/accelerations/pilot/x-accel-fps_sec</node>
      </chunk>

      <chunk>
			<name>y-accel</name>
			<type>double</type>
			<node>/accelerations/pilot/y-accel-fps_sec</node>
      </chunk>

      <chunk>
			<name>z-accel</name>
			<type>double</type>
			<node>/accelerations/pilot/z-accel-fps_sec</node>
      </chunk>

	<!-- ##### imu_orientation 3 doubles ##### -->
     <chunk>
			<name>roll</name>
			<type>double</type>
			<node>/orientation/roll-deg</node>
      </chunk>

	  <chunk>
			<name>pitch</name>
			<type>double</type>
			<node>/orientation/pitch-deg</node>
      </chunk>

      <chunk>
			<name>heading</name>
			<type>double</type>
			<node>/orientation/heading-deg</node>
      </chunk>

	<chunk>
			<name>latitude</name>
			<type>double</type>
			<node>/position/latitude-deg</node>
      </chunk> 

      <chunk>
			<name>longitude</name>
			<type>double</type>
			<node>/position/longitude-deg</node>
      </chunk> 

	   <chunk>
			<name>altitude</name>
			<type>double</type>
			<node>/position/altitude-ft</node>
      </chunk>   


      

      

      
    </output>

  </generic>

</PropertyList>
